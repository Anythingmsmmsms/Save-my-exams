{
    "name": "SaveMyExams Crack",
    "version": "1.0.0",
    "description": "Unlock some paid features for free",
    "manifest_version": 3,
    "author": "Stefanuk12",
    "declarative_net_request": {
        "rule_resources": [{
            "id": "ruleset_1",
            "enabled": true,
            "path": "rules.json"
        }]
    },
    "background": {
        "service_worker": "background.js",
        "type": "module"
    },
    "permissions": [
        "declarativeNetRequest",
        "scripting"
    ],
    "host_permissions": [
        "https://*.savemyexams.com/*"
    ]
}